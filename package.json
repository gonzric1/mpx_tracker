{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.1.0",
    "@hotwired/turbo-rails": "^7.1.3",
    "@popperjs/core": "^2.11.5",
    "@rails/actiontext": "^7.0.3-1",
    "bootstrap": "^5.2.0",
    "esbuild": "^0.14.53",
    "font-awesome": "^4.7.0",
    "sass": "^1.54.3",
    "trix": "^2.0.0-beta.0"
  },
  "scripts": {
    "build:css": "sass ./app/assets/stylesheets/application.bootstrap.scss ./app/assets/builds/application.css --no-source-map --load-path=node_modules -I node_modules/font-awesome/css",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets"
  }
}
